<div class="modal fade" id="modal-preset-delete" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="exampleModalLabel"><%= lang.presets.modals.delete_title %></h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>

            <div class="modal-body">
                <div>
                    <%= lang.presets.modals.delete_text.split("%1")[0] %><span id="p-delete-name"></span><%= lang.presets.modals.delete_text.split("%1")[1] %>
                </div>
            </div>

            <div class="modal-footer">
                <div>
                    <button type="button" class="btn btn-danger me-2" id="btn-preset-delete-confirm"><%= lang.presets.modals.delete_button %></button>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"><%= lang.presets.modals.close %></button>
                </div>
            </div>
        </div>
    </div>
</div>